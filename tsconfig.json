{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
     "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
       "@public/*": [
        "./public/*"
      ],
      "@*": [
        "./src/*"
      ],
      "@mui/*": [
        "./src/mui/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": ["src/styles/*"],
      "types/*": [
        "src/types/*"
      ]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts","types"],
  "exclude": ["node_modules"]
}
